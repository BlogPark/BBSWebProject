    <script src="@Url.Content("/js/jquery.flot.js")"></script>
    <script src="@Url.Content("/js/jquery.flot.pie.js")"></script>
    <script src="@Url.Content("/js/jquery.flot.orderBars.js")"></script>
    <script src="@Url.Content("/js/jquery.flot.resize.js")"></script>
    <script src="@Url.Content("/js/kaifanewsjs.js")"></script>
    @RenderSection("bodyjs", false)
</body>
</html>
