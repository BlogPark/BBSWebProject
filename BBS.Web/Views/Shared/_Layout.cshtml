        <div class="navbar-inner">

            <div class="container">

                <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>

                <a class="brand" href="/Home">了解程序猿世界</a>
                @Html.Partial("_TopUserPartial")
            </div> <!-- /container -->

        </div> <!-- /navbar-inner -->

    </div> <!-- /navbar -->
    <div id="content">

        <div class="container">

            <div class="row">

                <div class="span3" id="myScrollspy">

                    <div class="account-container">

                        @*<div class="account-avatar">
                                <img src="./img/headshot.png" alt="" class="thumbnail" />
                            </div>*@ <!-- /account-avatar -->

                        <div class="account-details">

                            <span class="account-name">开发新闻</span>

                            <span class="account-role">搜集最新编程信息</span>

                            @*<span class="account-actions">
                                    <a href="javascript:;">Profile</a> |

                                    <a href="javascript:;">Edit Settings</a>
                                </span>*@

                        </div> <!-- /account-details -->

                    </div> <!-- /account-container -->
                    <hr />
                    <ul id="main-nav" class="nav nav-tabs nav-stacked">

                        <li class="active">
                            <a href="/Home/Index">
                                <i class="icon-home"></i>
                                网站首页
                            </a>
                        </li>

                        <li>
                            <a href="/Home/CategoryPage?cateid=2">
                                <i class="icon-pushpin"></i>
                                软件开发
                            </a>
                        </li>

                        <li>
                            <a href="/Home/CategoryPage?cateid=3">
                                <i class="icon-th-list"></i>
                                互联网新态
                            </a>
                        </li>

                        <li>
                            <a href="/Home/CategoryPage?cateid=4">
                                <i class="icon-th-large"></i>
                                游  戏
