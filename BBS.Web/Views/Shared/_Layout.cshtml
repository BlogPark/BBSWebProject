<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="/imgart/kaifanews.ico" type="image/ico" rel="shortcut icon">
    <meta name="author" content="开发新闻网">
    <meta name="baidu_union_verify" content="a67a72d12896233163b11b4f684ad4bb">
    <title>@ViewBag.Title</title>
    <meta name="keywords" content="@ViewBag.KeyWord">
    <meta name="description" content="@ViewBag.Description">
    <link href="@Url.Content("/css/bootstrap.min.css")" rel="stylesheet" />
    <link href="@Url.Content("/css/bootstrap-responsive.min.css")" rel="stylesheet" />
    <link href="@Url.Content("/css/adminia.css")" rel="stylesheet" />
    <link href="@Url.Content("/css/adminia-responsive.css")" rel="stylesheet" />
    <link href="@Url.Content("/css/pages/dashboard.css")" rel="stylesheet" />
    <link href="@Url.Content("/css/font-awesome.css")" rel="stylesheet" />
