<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="@Url.Content("/css/bootstrap.min.css")">
    ><link rel="stylesheet" href="@Url.Content("/css/bootstrap-theme.min.css")">
    ><link rel="stylesheet" href="@Url.Content("/css/kaifanews.css")">
    <script src="@Url.Content("/js/jquery-2.1.4.min.js")"></script>
    <script src="@Url.Content("/js/bootstrap.min.js")"></script>
    <script src="@Url.Content("/js/kaifanews.js")"></script>
</head>
<body>
    @Html.Action("Header", "Public", new { area = "" });
    @RenderSection("navscript", required: false)
    @RenderBody()
    @Html.Action("Footer", "Public", new { area = "" });
    @RenderSection("footerscript", required: false)
</body>
</html>
